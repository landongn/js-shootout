{
	"name": "rosy",
	"description": "An inheritable / extendable JavaScript framework.",
	"repository": {
		"type": "git",
		"url": "https://github.com/ff0000/rosy.git"
	},
	"dependencies": {
		"jshint": ">=0.9.x",
		"requirejs": ">=2.0.x",
		"colors": ">=0.6.x",
		"wrench": ">=1.3.x"
	},
	"devDependencies": {
		"mocha": "1.3.x",
		"union": ">=0.3.4",
		"ecstatic": ">=0.1.6",
		"optimist": ">=0.3.4"
	},
	"systemDependencies": {
		"node": {
			"version": ">=0.9",
			"install": "brew update && brew install node",
			"upgrade": "brew update && brew upgrade node"
		},
		"npm": {
			"version": ">= 1.1.59",
			"install": "brew update && brew install node",
			"upgrade": "brew update && brew upgrade node"
		}
	},
	"scripts" : {
		"install" : "./config/init/rosy.js",
		"update" : "./config/init/rosy.js"
	},
	"config": {
		"scope": "project/source/js",
		"replaceVars": true
	}
}
